Object subclass: #AlgoRemis	instanceVariableNames: 'choferes viajes'	classVariableNames: ''	package: 'TP1'!!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/13/2021 01:20'!registrarChoferDeAuto: unNombre conTarifaInicial: unaTarifaInicial	(unaTarifaInicial < 0) ifTrue: [ TarifaInicialError signal ].	choferes add: (Chofer new nombre: unNombre tarifaInicial: unaTarifaInicial).! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/13/2021 01:39'!crearViajeConDestino: unDestino aKms: totalKilometros peajes: totalPeajes	(totalKilometros <= 0) ifTrue: [ CantidadDeKilometrosError signal ].	(totalPeajes < 0) ifTrue: [ CantidadDePeajesError signal ].		viajes at: unDestino put: (Viaje new destino: unDestino kilometros: totalKilometros peajes: totalPeajes).	(unDestino includesSubstring: 'Hospital') ifTrue: [ 		viajes at: unDestino put: (ViajeHospital new destino: unDestino kilometros: totalKilometros peajes: totalPeajes).		 ].! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/13/2021 02:35'!viajeMasBaratoParaDestino: unDestino	[ (viajes at: unDestino) establecerChofer: (self obtenerChoferMasBarato: unDestino)]	on: KeyNotFound do: [ DestinoInexistenteError signal ].		^ (viajes at: unDestino)! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/13/2021 01:31'!registrarChoferDeAutoElectrico: unNombre conTarifaInicial: unaTarifaInicial	(unaTarifaInicial < 0) ifTrue: [ TarifaInicialError signal ].	choferes add: (ChofeDeAutoElectrico new nombre: unNombre tarifaInicial: unaTarifaInicial).! !!AlgoRemis methodsFor: 'initialization' stamp: 'MartinSchipani 10/8/2021 16:40'!initialize	choferes := OrderedCollection new.	viajes := Dictionary new.! !!AlgoRemis methodsFor: 'private' stamp: 'MartinSchipani 10/13/2021 02:31'!obtenerChoferMasBarato: unDestino 	| precioMasBarato choferMasBarato |					precioMasBarato := (viajes at: unDestino) consultarPrecioCon: (choferes at: 1).	choferMasBarato := choferes at: 1.	choferes do: [ : chofer | 		((viajes at: unDestino) consultarPrecioCon: chofer) < precioMasBarato ifTrue: [ choferMasBarato := chofer ].		 ].	^ choferMasBarato ! !Object subclass: #Chofer	instanceVariableNames: 'nombre tarifaInicial tarifaPorKilometro tarifaPorPeaje'	classVariableNames: ''	package: 'TP1'!!Chofer methodsFor: 'accessing' stamp: 'MartinSchipani 10/8/2021 04:00'!nombre	^ nombre! !!Chofer methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 16:58'!calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes 	^ tarifaInicial + (kilometros * tarifaPorKilometro) + (peajes * tarifaPorPeaje)! !!Chofer methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 14:09'!nombre: unNombre tarifaInicial: unaTarifaInicial 	tarifaPorKilometro := 50.	tarifaPorPeaje := 100.	nombre := unNombre.	tarifaInicial := unaTarifaInicial.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Chofer class	instanceVariableNames: ''!!Chofer class methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 16:44'!nombre: unNombre tarifaInicial: unaTarifaInicial	| chofer |.	chofer := self new.	chofer nombre: unNombre tarifaInicial: unaTarifaInicial.	^ chofer! !Chofer subclass: #ChofeDeAutoElectrico	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!ChofeDeAutoElectrico methodsFor: 'accessing' stamp: 'MartinSchipani 10/8/2021 14:19'!nombre	^ nombre! !!ChofeDeAutoElectrico methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 16:58'!calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes 	^ tarifaInicial + (kilometros * tarifaPorKilometro) + (peajes * tarifaPorPeaje)! !!ChofeDeAutoElectrico methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 14:17'!nombre: unNombre tarifaInicial: unaTarifaInicial	tarifaPorKilometro := 30.	tarifaPorPeaje := 50.	nombre := unNombre.	tarifaInicial := unaTarifaInicial.! !Object subclass: #Viaje	instanceVariableNames: 'destino kilometros peajes chofer'	classVariableNames: ''	package: 'TP1'!!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 18:53'!precio	^ chofer calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 03:59'!chofer	^ chofer nombre! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 18:53'!destino: unDestino kilometros: totalKilometros peajes: totalPeajes 	destino := unDestino.	kilometros := totalKilometros.	peajes := totalPeajes.! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 17:55'!consultarPrecioCon: unChofer	^ unChofer calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 03:56'!establecerChofer: unChofer 	chofer := unChofer.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Viaje class	instanceVariableNames: ''!!Viaje class methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 16:44'!destino: unDestino kilometros: totalKilometros peajes: totalPeajes	| viaje |.	viaje := self new.	viaje destino: unDestino kilometros: totalKilometros peajes: totalPeajes.	^ viaje! !Viaje subclass: #ViajeHospital	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!ViajeHospital methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 18:58'!precio	^ (chofer calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes) * 0.2! !