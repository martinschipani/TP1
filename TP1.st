Object subclass: #AlgoRemis	instanceVariableNames: 'choferes viajes'	classVariableNames: ''	package: 'TP1'!!AlgoRemis methodsFor: 'private' stamp: 'MartinSchipani 10/9/2021 19:16'!obtenerChoferMasBarato: unDestino 	| precioMasBarato choferMasBarato |	precioMasBarato := (viajes at: unDestino) consultarPrecioCon: (choferes at: 1).	choferMasBarato := choferes at: 1.	choferes do: [ : chofer | 		((viajes at: unDestino) consultarPrecioCon: chofer) < precioMasBarato ifTrue: [ choferMasBarato := chofer ].		 ].	^ choferMasBarato ! !!AlgoRemis methodsFor: 'initialization' stamp: 'MartinSchipani 10/8/2021 16:40'!initialize	choferes := OrderedCollection new.	viajes := Dictionary new.! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 03:43'!registrarChoferDeAuto: unNombre conTarifaInicial: unaTarifaInicial 	choferes add: (Chofer new nombre: unNombre tarifaInicial: unaTarifaInicial).! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 18:55'!crearViajeConDestino: unDestino aKms: totalKilometros peajes: totalPeajes	viajes at: unDestino put: (Viaje new destino: unDestino kilometros: totalKilometros peajes: totalPeajes).	(unDestino includesSubstring: 'Hospital') ifTrue: [ 		viajes at: unDestino put: (Hospital new destino: unDestino kilometros: totalKilometros peajes: totalPeajes).		 ].! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 19:15'!viajeMasBaratoParaDestino: unDestino		(viajes at: unDestino) establecerChofer: (self obtenerChoferMasBarato: unDestino).	^ (viajes at: unDestino)! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 14:19'!registrarChoferDeAutoElectrico: unNombre conTarifaInicial: unaTarifaInicial 	choferes add: (ChofeDeAutoElectrico new nombre: unNombre tarifaInicial: unaTarifaInicial).! !Object subclass: #Chofer	instanceVariableNames: 'nombre tarifaInicial tarifaPorKilometro tarifaPorPeaje'	classVariableNames: ''	package: 'TP1'!!Chofer methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 14:09'!nombre: unNombre tarifaInicial: unaTarifaInicial 	tarifaPorKilometro := 50.	tarifaPorPeaje := 100.	nombre := unNombre.	tarifaInicial := unaTarifaInicial.! !!Chofer methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 16:58'!calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes 	^ tarifaInicial + (kilometros * tarifaPorKilometro) + (peajes * tarifaPorPeaje)! !!Chofer methodsFor: 'accessing' stamp: 'MartinSchipani 10/8/2021 04:00'!nombre	^ nombre! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Chofer class	instanceVariableNames: ''!!Chofer class methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 16:44'!nombre: unNombre tarifaInicial: unaTarifaInicial	| chofer |.	chofer := self new.	chofer nombre: unNombre tarifaInicial: unaTarifaInicial.	^ chofer! !Chofer subclass: #ChofeDeAutoElectrico	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!ChofeDeAutoElectrico methodsFor: 'accessing' stamp: 'MartinSchipani 10/8/2021 14:19'!nombre	^ nombre! !!ChofeDeAutoElectrico methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 14:17'!nombre: unNombre tarifaInicial: unaTarifaInicial	tarifaPorKilometro := 30.	tarifaPorPeaje := 50.	nombre := unNombre.	tarifaInicial := unaTarifaInicial.! !!ChofeDeAutoElectrico methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 16:58'!calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes 	^ tarifaInicial + (kilometros * tarifaPorKilometro) + (peajes * tarifaPorPeaje)! !Object subclass: #Viaje	instanceVariableNames: 'destino kilometros peajes chofer'	classVariableNames: ''	package: 'TP1'!!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 17:55'!consultarPrecioCon: unChofer	^ unChofer calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 18:53'!destino: unDestino kilometros: totalKilometros peajes: totalPeajes 	destino := unDestino.	kilometros := totalKilometros.	peajes := totalPeajes.! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 03:59'!chofer	^ chofer nombre! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 03:56'!establecerChofer: unChofer 	chofer := unChofer.! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 18:53'!precio	^ chofer calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Viaje class	instanceVariableNames: ''!!Viaje class methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 16:44'!destino: unDestino kilometros: totalKilometros peajes: totalPeajes	| viaje |.	viaje := self new.	viaje destino: unDestino kilometros: totalKilometros peajes: totalPeajes.	^ viaje! !Viaje subclass: #Hospital	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Hospital methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 18:58'!precio	^ (chofer calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes) * 0.2! !