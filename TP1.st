Object subclass: #AlgoRemis	instanceVariableNames: 'choferes viajes'	classVariableNames: ''	package: 'TP1'!!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/14/2021 01:40'!registrarChoferDeAutoElectrico: unNombre conTarifaInicial: unaTarifaInicial	(unaTarifaInicial < 0) ifTrue: [ TarifaInicialError signal ].	choferes add: (ChoferDeAutoElectrico new nombre: unNombre tarifaInicial: unaTarifaInicial).! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/13/2021 01:39'!crearViajeConDestino: unDestino aKms: totalKilometros peajes: totalPeajes	(totalKilometros <= 0) ifTrue: [ CantidadDeKilometrosError signal ].	(totalPeajes < 0) ifTrue: [ CantidadDePeajesError signal ].		viajes at: unDestino put: (Viaje new destino: unDestino kilometros: totalKilometros peajes: totalPeajes).	(unDestino includesSubstring: 'Hospital') ifTrue: [ 		viajes at: unDestino put: (ViajeHospital new destino: unDestino kilometros: totalKilometros peajes: totalPeajes).		 ].! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/13/2021 02:35'!viajeMasBaratoParaDestino: unDestino	[ (viajes at: unDestino) establecerChofer: (self obtenerChoferMasBarato: unDestino)]	on: KeyNotFound do: [ DestinoInexistenteError signal ].		^ (viajes at: unDestino)! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/14/2021 02:02'!registrarChoferDeAuto: unNombre conTarifaInicial: unaTarifaInicial	(unaTarifaInicial < 0) ifTrue: [ TarifaInicialError signal ].	choferes add: (ChoferDeAuto new nombre: unNombre tarifaInicial: unaTarifaInicial).! !!AlgoRemis methodsFor: 'private' stamp: 'MartinSchipani 10/13/2021 02:31'!obtenerChoferMasBarato: unDestino 	| precioMasBarato choferMasBarato |					precioMasBarato := (viajes at: unDestino) consultarPrecioCon: (choferes at: 1).	choferMasBarato := choferes at: 1.	choferes do: [ : chofer | 		((viajes at: unDestino) consultarPrecioCon: chofer) < precioMasBarato ifTrue: [ choferMasBarato := chofer ].		 ].	^ choferMasBarato ! !!AlgoRemis methodsFor: 'initialization' stamp: 'MartinSchipani 10/8/2021 16:40'!initialize	choferes := OrderedCollection new.	viajes := Dictionary new.! !Error subclass: #CantidadDeKilometrosError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Error subclass: #CantidadDePeajesError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #Chofer	instanceVariableNames: 'nombre tarifaInicial'	classVariableNames: ''	package: 'TP1'!!Chofer methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/14/2021 02:05'!nombre: unNombre tarifaInicial: unaTarifaInicial 	nombre := unNombre.	tarifaInicial := unaTarifaInicial.! !!Chofer methodsFor: 'accessing' stamp: 'MartinSchipani 10/8/2021 04:00'!nombre	^ nombre! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Chofer class	instanceVariableNames: ''!!Chofer class methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 16:44'!nombre: unNombre tarifaInicial: unaTarifaInicial	| chofer |.	chofer := self new.	chofer nombre: unNombre tarifaInicial: unaTarifaInicial.	^ chofer! !Chofer subclass: #ChoferDeAuto	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!ChoferDeAuto methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/14/2021 02:48'!calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes 	^ tarifaInicial + (kilometros * 50) + (peajes * 100)! !Chofer subclass: #ChoferDeAutoElectrico	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!ChoferDeAutoElectrico methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/14/2021 02:49'!calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes 	^ tarifaInicial + (kilometros * 30) + (peajes * 50)! !Error subclass: #DestinoInexistenteError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Error subclass: #TarifaInicialError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #Viaje	instanceVariableNames: 'destino kilometros peajes chofer'	classVariableNames: ''	package: 'TP1'!!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 18:53'!precio	^ chofer calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 17:55'!consultarPrecioCon: unChofer	^ unChofer calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 03:59'!chofer	^ chofer nombre! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 18:53'!destino: unDestino kilometros: totalKilometros peajes: totalPeajes 	destino := unDestino.	kilometros := totalKilometros.	peajes := totalPeajes.! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 03:56'!establecerChofer: unChofer 	chofer := unChofer.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Viaje class	instanceVariableNames: ''!!Viaje class methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/9/2021 16:44'!destino: unDestino kilometros: totalKilometros peajes: totalPeajes	| viaje |.	viaje := self new.	viaje destino: unDestino kilometros: totalKilometros peajes: totalPeajes.	^ viaje! !Viaje subclass: #ViajeHospital	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!ViajeHospital methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/14/2021 02:51'!precio	^ (chofer calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes) * 0.2! !