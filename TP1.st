Object subclass: #AlgoRemis	instanceVariableNames: 'choferes viajes'	classVariableNames: ''	package: 'TP1'!!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 18:36'!viajeMasBaratoParaDestino: unDestino	| viajeMasBarato |.	viajeMasBarato := (viajes at: unDestino) establecerChofer: (choferes at: 1). 	choferes do: [ : chofer |		(viajes at: unDestino) establecerChofer: chofer.		(viajes at: unDestino) precio < (viajeMasBarato precio) ifTrue: [ viajeMasBarato := (viajes at: unDestino) ].		 ].	^ viajeMasBarato 	! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 14:19'!registrarChoferDeAutoElectrico: unNombre conTarifaInicial: unaTarifaInicial 	choferes add: (ChofeDeAutoElectrico new nombre: unNombre tarifaInicial: unaTarifaInicial).! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 03:43'!registrarChoferDeAuto: unNombre conTarifaInicial: unaTarifaInicial 	choferes add: (Chofer new nombre: unNombre tarifaInicial: unaTarifaInicial).! !!AlgoRemis methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 16:41'!crearViajeConDestino: unDestino aKms: totalKilometros peajes: totalPeajes 	viajes at: unDestino put: (Viaje new destino: unDestino kilometros: totalKilometros peajes: totalPeajes).! !!AlgoRemis methodsFor: 'initialization' stamp: 'MartinSchipani 10/8/2021 16:40'!initialize	choferes := OrderedCollection new.	viajes := Dictionary new.! !Object subclass: #ChofeDeAutoElectrico	instanceVariableNames: 'nombre tarifaInicial tarifaPorKilometro tarifaPorPeaje'	classVariableNames: ''	package: 'TP1'!!ChofeDeAutoElectrico methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 16:58'!calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes 	^ tarifaInicial + (kilometros * tarifaPorKilometro) + (peajes * tarifaPorPeaje) ! !!ChofeDeAutoElectrico methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 14:17'!nombre: unNombre tarifaInicial: unaTarifaInicial	tarifaPorKilometro := 30.	tarifaPorPeaje := 50.	nombre := unNombre.	tarifaInicial := unaTarifaInicial.! !!ChofeDeAutoElectrico methodsFor: 'accessing' stamp: 'MartinSchipani 10/8/2021 14:19'!nombre	^ nombre! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!ChofeDeAutoElectrico class	instanceVariableNames: ''!!ChofeDeAutoElectrico class methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 14:15'!nombre: unNombre tarifaInicial: unaTarifaInicial	| choferDeAutoElectrico |	choferDeAutoElectrico := self new.	choferDeAutoElectrico nombre: unNombre tarifaInicial: unaTarifaInicial.	^ choferDeAutoElectrico! !Object subclass: #Chofer	instanceVariableNames: 'nombre tarifaInicial tarifaPorKilometro tarifaPorPeaje'	classVariableNames: ''	package: 'TP1'!!Chofer methodsFor: 'accessing' stamp: 'MartinSchipani 10/8/2021 04:00'!nombre	^ nombre! !!Chofer methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 16:56'!calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes 	^ tarifaInicial + (kilometros * tarifaPorKilometro) + (peajes * tarifaPorPeaje) ! !!Chofer methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 14:09'!nombre: unNombre tarifaInicial: unaTarifaInicial 	tarifaPorKilometro := 50.	tarifaPorPeaje := 100.	nombre := unNombre.	tarifaInicial := unaTarifaInicial.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Chofer class	instanceVariableNames: ''!!Chofer class methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 03:30'!nombre: unNombre tarifaInicial: unaTarifaInicial	| chofer |	chofer := self new.	chofer nombre: unNombre tarifaInicial: unaTarifaInicial.	^ chofer! !Object subclass: #Viaje	instanceVariableNames: 'destino kilometros peajes chofer'	classVariableNames: ''	package: 'TP1'!!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 16:54'!precio	^ chofer calcularPrecioCantidadKilometros: kilometros cantidadPeajes: peajes! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 03:59'!chofer	^ chofer nombre! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 03:56'!establecerChofer: unChofer 	chofer := unChofer.! !!Viaje methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/8/2021 14:09'!destino: unDestino kilometros: totalKilometros peajes: totalPeajes 	destino := unDestino.	kilometros := totalKilometros.	peajes := totalPeajes.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Viaje class	instanceVariableNames: ''!!Viaje class methodsFor: 'as yet unclassified' stamp: 'Anonymous 10/8/2021 13:56'!destino: unDestino kilometros: totalKilometros peajes: totalPeajes	| viaje |	viaje := self new.	viaje destino: unDestino kilometros: totalKilometros peajes: totalPeajes.	^ viaje! !